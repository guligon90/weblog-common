{
  "name": "@weblogapp/weblogapp-common",
  "version": "0.0.10",
  "description": "",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "del ./build/*",
    "commit-msg": "commitlint --edit $1",
    "doctoc": "doctoc ./README.md --github",
    "lint": "eslint . --ext .cjs,.js,.ts",
    "lint:fix": "eslint . --ext .cjs,.js,.ts --fix",
    "pre-commit": "lint-staged",
    "prettier": "prettier --config .prettierrc.cjs '**/*.{js,ts,tsx,json,css,scss,md}' --write",
    "pub": "yarn version --patch && yarn build && yarn publish"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "nats",
    "sdk",
    "typescript",
    "weblog",
    "microservices",
    "middlewares",
    "express",
    "REST",
    "event-driven",
    "swagger"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "del-cli": "^4.0.0",
    "doctoc": "^2.1.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@types/cookie-session": "^2.0.42",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.1",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "jsonwebtoken": "^8.5.1",
    "node-nats-streaming": "^0.3.2",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "winston": "^3.6.0"
  }
